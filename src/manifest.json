{
  "name": "GStamps",
  "description": "Stickers for google meet",
  "version": "0.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "declarativeContent"
  ],
  "host_permissions": [
    "https://tenor.googleapis.com/v2/*",
    "https://c.tenor.com/*"
  ],
  "action" : {
    "default_popup" : "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "exclude_matches": ["https://meet.google.com/"],
      "css": ["my-styles.css"],
      "js": ["twemoji.min.js", "keys.js", "replacer.js"]
    }
  ]
}
